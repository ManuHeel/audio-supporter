<div class="row">
  <form>
    <div class="row">
      <div class="input-field col s10">
        <div class="autocomplete" id="tags">
          <div class="ac-tags"></div>
          <div class="ac-input">
            <input type="text" id="tagsInput" placeholder="Search by tags" data-activates="tagsDropdown" data-beloworigin="true" autocomplete="off">
          </div>
          <ul id="tagsDropdown" class="dropdown-content ac-dropdown"></ul>
          <input type="hidden" name="tagsHidden" />
        </div>
        <label class="active" for="tagsInput">Search : </label>
      </div>
    </div>
    <button class="btn waves-effect waves-light" type="submit" name="action">Search
      <i class="material-icons right">search</i>
    </button>
  </form>
</div>

<script src="../jquery.materialize-autocomplete.js"></script>

</script>
